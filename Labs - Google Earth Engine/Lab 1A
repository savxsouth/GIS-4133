// Complex Container
var my_dict = {'college': 'OU', 'color': 'crimson', 'number': my_variable};
// Square brackets can be used to access dictionary items by key.
print(my_dict['color']);
// Or you can use the dot notation to get the same result.
print(my_dict.color);
print(my_dict.number);

